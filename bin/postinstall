#!/bin/bash
script_dir=$(dirname $(readlink -f $0))
base_dir=$(dirname $script_dir)
cd $script_dir/../../../node_modules/.bin
ln -sf ../@drumee/server-dev-tools/bin/watch.sh ./drumee-server-devel
ln -sf ../@drumee/server-dev-tools/bin/watch.sh ./drumee-server-deploy
cp -r $base_dir/.dev-tools.rc $script_dir/../../../
